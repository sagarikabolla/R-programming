_________________________________________
x=1:10
y1=x
y2=x^2
y3=sqrt(x)
plot(x,y1,type="l",col="blue",ylim=range(0,y2,y3),xlab="x_axis",ylab="y_axis")
lines(x,y2,type="l",col="red")
lines(x,y3,type="l",col="green")
legend("topright",legend=c("line1","line2","line3"),col=c("blue","red","green"),lty=1)
title("Multiple lines plot")
